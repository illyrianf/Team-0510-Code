#include <kipr/wombat.h>
#include <stdio.h>

#define MOTORl 1
#define MOTORr 0
#define frontSERVO 3
#define smallSERVO 2
#define tubeGRABBER 0


void triplegrrr(void) {

	set_servo_position(frontSERVO, 300);    
	msleep(300);
   	
	mav(MOTORr, 1000);
	mav(MOTORl, 1000);
	msleep(600);

	set_servo_position(smallSERVO, 1900);
	msleep(300);

	set_servo_position(frontSERVO, 1500);
	msleep(300);

	set_servo_position(smallSERVO, 0);
	msleep(300);

	set_servo_position(frontSERVO, 500);
	msleep(300);

}

void triplerggg(void) {

	set_servo_position(frontSERVO, 1700);    
	msleep(300);
    
	mav(MOTORr, 1000);
	mav(MOTORl, 1000);
	msleep(600);

	set_servo_position(smallSERVO, 1900);
	msleep(300);
	
	set_servo_position(frontSERVO, 500);
	msleep(300);

	set_servo_position(smallSERVO, 0);
	msleep(300);

	set_servo_position(frontSERVO, 1500);
	msleep(300);

}
	

void movepompomsright(void) {

	set_servo_position(frontSERVO, 500);    
	msleep(300);
   	
	mav(MOTORr, 1000);
	mav(MOTORl, 1000);
	msleep(600);
    
	set_servo_position(frontSERVO, 1500);
	msleep(300);

}

void movepompomsleft(void) {
    
	set_servo_position(frontSERVO, 1500);    
	msleep(300);

	mav(MOTORr, 1000);
	mav(MOTORl, 1000);
	msleep(600);
    
	set_servo_position(frontSERVO, 500);
	msleep(300);

}

void sortsalternatingpompoms(void){

	movepompomsright();
	movepompomsleft();
	movepompomsright();
	movepompomsleft();
	movepompomsright();
	movepompomsleft();
	movepompomsright();
	movepompomsleft();
	triplegrrr();
	triplerggg();

}

int main(){

	enable_servos();

	sortsalternatingpompoms();

	ao();
    
	return 0;
    
}
